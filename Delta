SELECT
  creation_time,
  user_email,
  statement_type,
  job_id,
  query
FROM `region-<REGION>`.INFORMATION_SCHEMA.JOBS_BY_PROJECT
WHERE statement_type IN ('DELETE','MERGE')
  AND query LIKE '%act_info%'            -- adjust if your table name differs
  AND creation_time >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY)
ORDER BY creation_time DESC;

protoPayload.serviceName="bigquery.googleapis.com"
protoPayload.methodName="google.cloud.bigquery.v2.JobService.InsertJob"
protoPayload.metadata.@type="type.googleapis.com/google.cloud.audit.BigQueryAuditMetadata"
protoPayload.metadata.jobChange.job.jobConfig.queryConfig.statementType="DELETE"
protoPayload.metadata.jobChange.job.jobConfig.queryConfig.query:"`PROJECT_ID.DATASET.act_info`"
timestamp >= "2025-08-25T00:00:00Z"
