project_id   = "your-project-id"
env_name     = "your-composer-env-name"    # optional, if you want to pin to one env
start        = context["data_interval_start"].strftime("%Y-%m-%dT%H:%M:%SZ")
end          = context["data_interval_end"].strftime("%Y-%m-%dT%H:%M:%SZ")

log_filter = (
    'resource.type="cloud_composer_environment" AND '
    f'resource.labels.environment_name="{env_name}" AND '
    # only Airflow logs:
    f'logName=~"projects/{project_id}/logs/airflow.*" AND '
    # your data‐interval window:
    f'timestamp >= "{start}" AND timestamp < "{end}"'
)
